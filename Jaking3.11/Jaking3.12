def progress_bar(task="Processing", duration=2):
    """Enhanced progress bar with better visuals"""
    for i in range(21):
        percent = int((i/20) * 100)
        bar = "â–ˆ" * i + "â–‘" * (20 - i)
        sys.stdout.write(f"\r{Colors.CYAN}{task}: |{bar}| {percent}%{Colors.RESET}")
        sys.stdout.flush()
        time.sleep(duration / 20)
    print()

